              "code": "searchData ={\n  success: function(response){\n    $.each(response.submissions, function(index, submission) {\n      K.api(\n        \"PUT\", \n        \"/app/api/v1/submissions/\"+submission.id,\n        updateData\n      );\n    })\n  },\n  error: function(e){\n    alert(e.responseJSON.error);\n  },\n  complete: function(){alert(\"Records have been canceled\")}\n  }\n      \nupdateData ={\n  data: JSON.stringify({\n      \"coreState\": \"Closed\",\n      \"values\": {\n        \"Status\":\"Cancelled\",\n        \"Cancellation Reason\":\"Test\"\n      }\n  }) ,\n  success: function(response){},\n  error: function(e){alert(e.responseJSON.error)}\n}\n\nvar query = K('field[Query]').value()!= null ? K('field[Query]').value()  : ''\nvar q = query != '' ? query + ' && values[Assigned Team]=\"'+K('field[Assigned Team]').value() +'\"' : 'values[Assigned Team]=\"'+K('field[Assigned Team]').value()+'\"'\nK.api(\n  'GET',\n  'app/api/v1/kapps/queue/forms/'+K('field[Form Slug]').value()+'/submissions?coreState='+K('field[State]').value()+'&timeline=createdAt&direction=DESC&limit='+K('field[Limit]').value()+'&q='+q+'\"&include=values',\n  searchData\n)\n",
